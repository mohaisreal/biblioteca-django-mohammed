{
    "builds": [
      {
        "src": "locallibrary/wsgi.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.10.2" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "locallibrary/wsgi.py"
      }
    ],
    "buildCommand": "python3 manage.py makemigrations && python3 manage.py migrate"
}